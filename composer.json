{
    "name": "hi-folks/laravel-dashboard-devto-tile",
    "description": "Tile for Spatie Laravel Dashboard to list DEV.to articles",
    "keywords": [
        "hi-folks",
        "laravel-dashboard-devto-tile"
    ],
    "homepage": "https://github.com/hi-folks/laravel-dashboard-devto-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Roberto Butti",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "HiFolks\\DevtoTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HiFolks\\DevtoTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "HiFolks\\DevtoTile\\DevtoTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
